&lt;h3 id=&quot;2-字体&quot;&gt;&lt;a href=&quot;#2-字体&quot; class=&quot;headerlink&quot; title=&quot;2.字体&quot;&gt;&lt;/a&gt;2.字体&lt;/h3&gt;&lt;figure class=&quot;highlight plaintext&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td class=&quot;gutter&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;1&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;2&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;3&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;*斜体文本*&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;**粗体文本**&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;***粗斜体文本***&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;

